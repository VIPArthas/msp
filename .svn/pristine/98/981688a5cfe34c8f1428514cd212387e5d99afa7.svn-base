package com.wh.util;

/**
 * 模板类型
 * @author 徐优优
 *
 */
public class StatusUtil {
	
	public static String templetStatusToStr(Integer status){
		String res = "";
		if(status == 1){
			res = "委托单";
		}else if(status == 2){
			res = "工艺卡";
		}else if(status == 3){
			res = "现场维修单";
		}else if(status == 4){
			res = "暗室处理单";
		}else if(status == 5){
			res = "委托报告";
		}else if(status == 6){
			res = "其他";
		}
		return res;
	}
	
	//委托单状态
	public static String entrustStatusToStr(Integer status){
		String res = "";
		if(status == 0){
			res = "正常";
		}else if(status == 1){
			res = "删除";
		}else if(status == 2){
			res = "已发送";
		}else if(status == 3){
			res = "暂存";
		}
		return res;
	}

}
