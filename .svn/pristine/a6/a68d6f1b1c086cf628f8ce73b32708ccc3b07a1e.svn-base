	private Date del_time;// 删除时间
	private String token;
	private String text;
	private String postList;// 岗位列表
	private String roleList;// 角色列表
	private String creator;//创建人id
	private Date create_time;//创建时间
	
	private List<String> deptList;//部门列表
	private String serviceName;//服务商名称
