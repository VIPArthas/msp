html,
body {
    height: 100%;
}
body {
    margin: 0;
    width: 100%;
}
a { text-decoration:none;}
nav a:active{-webkit-tap-highlight-color: transparent;}
nav a:hover{-webkit-tap-highlight-color: transparent;}
/*内容iframe*/
iframe {
    width: 100%;
    height: calc(100% - 50px);
    border: none;
}
/*底部nav*/
nav {
    height: 50px;
    width: 100%;
    position: fixed;
    bottom: 0px;
    background-color: #fff;
    border-top: 1px solid #E0E0E0;
}
nav a {
    float: left;
    height: 50px;
    width: 33.3%;
    color: gray;
    text-align: center;
    text-decoration: none;
}
nav a p {
    margin: 0px ;
    line-height: 20px;
    font-size: 12px;
    /*background-color: #00f;*/
    /*margin-bottom: 5px;*/
}
nav a span {
    display: block;
    width: 25px;
    height: 25px;
    margin: 0 auto;
    /*background-color: red;*/
    margin-top: 5px;
    background-image: url(/resource/xyuan/img/spr1.png);
    -webkit-background-size: auto 600%;
    background-size: auto 600%;
    /*background-position: 0 -150px;*/
}
/*nav字体颜色变化*/



nav .youyuan span {
    background-position: 0 -175px;
}

nav .select-actived span.you-yuan-ren {
    background-position: 0 -150px;
}
/*nav图标变化*/
nav .liaoyixai span {
    background-position: 0 -75px;
}

nav .select-actived span.liao-yi-xia {
    background-position: 0 -50px;
}
/*nav图标变化*/
nav .mymessage span {
    background-position: 0 -125px;
}

nav .select-actived span.wo-de {
    background-position: 0 -100px;
}

.yindaoone{background-image: url(/resource/xyuan/img/yindaoye1.jpg);background-repeat: no-repeat;background-size: 100% 100%;width: 100%;height: 100%}
.yindaotwo{background-image: url(/resource/xyuan/img/yindaoye2.jpg);background-repeat: no-repeat;background-size: 100% 100%;width: 100%;height: 100%}
.yindaotwo a{width: 6.8rem;height: 0.82rem;font-size: 0.3rem;color: white;line-height: 0.82rem;text-align: center; position: absolute;bottom: 1.5rem;background-color: #2abcb4;border-radius: 0.1rem;left: calc(50% - 3.4rem);}
