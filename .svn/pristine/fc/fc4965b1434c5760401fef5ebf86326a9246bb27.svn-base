@charset "utf-8";
/* CSS Document */
body,html{min-height:99%; font-family: "微软雅黑"}
body{background:url(../images/p_bg.png) no-repeat right bottom;overflow-y:auto}
.red{color:red;}
.mar-t-10{margin-top:10px}
.mar-b-10{margin-bottom:10px}
.main{margin:10px 10px 0 10px;font-family:"Arial";}
.search_box{background:#f0f0f0;padding:5px 0 5px 20px;color:#757575;}
.search_box table th{text-align:right;padding-right:10px}
.search_box table th,.search_box table td{padding:5px 0;font-family:Microsoft YaHei;font-size:12px;}
.search_box input.txt{border:1px solid #c4c4c4;line-height:24px;height:24px;width:140px}
.search_box input{vertical-align:middle}
.search_box label{padding-right:15px}
.search_box .btn{padding:0 18px;height:28px;line-height:28px;background: #00a65a}
.btn_box{background:#f0f0f0}
.btn_gncd{width:100%;height:34px;background:#f0f0f0;margin-top:10px}
.btn_box input{padding:0 10px 0 35px;float:left;list-style:none;color:#fff;height:34px;display:block;line-height:32px;border:none;margin-right:3px;display:block;background-image:url(../images/btn_icon.png);background-repeat:no-repeat}
.btn_tj{background-position:7px -125px;background-color:#090}
.btn_sc{background-color:#17A9FF;background-position:7px -213px}
.btn_bj{background-color:#17a9ff;background-position:9px -38px}
.btn_sx{background-color:#eb9526;background-position:9px -417px}
.btn_cz{background-color:#eb9526;background-position:9px -389px}
.btn_by_a{background-color:#934bf5;background-position:7px -82px}
.btn_by_b{background-color:#7b70f0;background-position:7px -169px}
.btn_by_c{background-color:#90c400;background-position:7px 7px}
.btn_by_d{background-color:#0084cc;background-position:7px -125px}
.main_table{}
.table{clear:both;font-family:"Arial";font-size:12px;}
.table tr th{color:#fff;background: #00a65a ;height:32px;line-height:16px; border-left:1px solid #008548;font-weight:normal; text-align:center;padding-left:0; font-size: 14px;}
.table tr td{border-bottom:1px solid #e9e9e9;padding:8px 5px;color:#565656;border-top:1px solid #FFFFFF}
.table tr.odd td{background:#ffffff}
.table tr.even td{background:#f6f6f6}
.table tr.hover td{background:#bcd4ec}
.table tr td a{color:#00a65a;}
.table tr td a:hover{color:#008548;text-decoration:underline;}
.table tr th a{ color: #FFFBCC}

/*layer*/
.layer_table{margin:8px auto 0;font-family:"Arial";font-size:12px;}
.layer_table,.layer_table th,.layer_table td{ border-collapse:collapse;border:1px solid #dddddd;padding:5px;}
.layer_table th{background:#f6f9ff; text-align:center;color:#666;}
.layer_table strong{font-weight:bold;}
.layer_txt,select,textarea,.layer_table .combo{border:1px solid #dddddd;}
.layer_table .datebox{border-color:#dddddd;}
.layer_table td label{color:#686868;}
textarea{font-size:12px;}
select{padding:5px;}
.ht-auto{height:auto;}
.layer_txt{height:26px;line-height:26px;}
.w100{width:100px;}
.w150{width:150px;}
.w200{width:200px;}
.w250{width:250px;}
.w380{width:380px;}
.w98p{width:98%;}
.w100p{width:100%;}
.btn_gr {text-align:right;padding:10px 15px 10px;}
.btn_gr .primarybtn,.btn_gr .graybtn{padding:4px 15px;margin-left:10px; }
.btn_gr .primarybtn{color:#fff;background:#71c7e8;border:1px solid #55accd;}
.btn_gr .graybtn{color:#fff;background:#c7c7c7;border:1px solid #adadad;}
.tem_area{width:521px;height: 61px;}
.upload_group{display:inline-block;margin-right:8px;*display:inline;*zoom:1;width:52px;height:24px;overflow:hidden;}
.upload_group .upld{position:absolute;height:24px;left:0px;top:0px;opacity:0;filter:alpha(opacity=0);z-index:9999;}
.upload_group .skewbtn{background:#19a9ff;width:52px;height:24px;color:#fff;border-radius:1px;}
.preview_pic li{float:left;margin-right:10px;position:relative;}
.preview_pic li .remove{display:block;position:absolute;width:19px;height:19px;background:url(../images/delete.png) no-repeat;text-indent:999em;overflow:hidden;right:0px;top:0px;}
.status_bar{background:#f0f0f0;padding:5px 0 5px 10px;color:#565656;font-family:"Arial";font-size:12px;line-height:28px;}

.main_txt{border:1px solid #cbd9f0;height:22px;line-height:22px;}
.table a.addrow,.table a.addrow:hover{text-decoration:underline;color:#006bd7;}
.enter_order .status_bar .write{width:18px;height:18px;margin-bottom: -4px;display:inline-block;margin-right:6px;background:url(../images/sys_icon.png) 0 -105px no-repeat;}
.enter_order .status_bar .paper{width:18px;height:18px;margin-bottom: -4px;display:inline-block;margin-right:6px;background:url(../images/sys_icon.png) 0 -137px no-repeat;}
.enter_order .status_bar .tv{width:18px;height:18px;margin-bottom: -4px;display:inline-block;margin-right:6px;background:url(../images/sys_icon.png) 0 -160px no-repeat;}
.enter_order .status_bar .son{width:18px;height:18px;margin-bottom: -4px;display:inline-block;margin-right:6px;background:url(../images/sys_icon.png) 0 -180px no-repeat;}
.enter_order .status_bar .moon{width:18px;height:18px;margin-bottom: -4px;display:inline-block;margin-right:6px;background:url(../images/sys_icon.png) 0 -202px no-repeat;}
.enter_order .status_bar .pin{width:18px;height:18px;margin-bottom: -4px;display:inline-block;margin-right:6px;background:url(../images/sys_icon.png) 0 -224px no-repeat;}
.enter_order .status_bar{background:#537bd0;color:#fff;height:20px;line-height:20px;}
.order_table{background:#ffffff;padding:10px;}
.order_table table,.order_table td,.order_table th{border-collapse:collapse;border:1px solid #dddddd;background:#f6f9ff;font-family:"Arial";font-size:12px;line-height:30px;}
.order_table .w100p{border:0px;width:100%;height:30px;line-height:30px; }
.order_table input.w100p{text-indent:7px;}
.order_table td label{padding-left:6px;}
.order_table th{background:#f6f9ff;color:#a19fa2;border-collapse:collapse;}
.order_table td{background:#fafbff;color:#616264;}
.order_table p{line-height:1.8;color:#666666;}
.page_btn{padding:25px 0 15px;text-align:right;}
.page_btn button{width:120px;height:36px;line-height:36px;display:inline-block;margin-left:15px;color:#fff;font-family:Microsoft YaHei;}
.purplebtn{background:#7a70ed;}
.orgbtn{background:#dc572e;}
.greenbtn{background:#0097aa;}
.bluebtn{background:#1e9ee9; vertial-align:middle;}
.disabled{pointer-events: none;background:#ccc!important;cursor: not-allowed;}
.page_btn button i{vertical-align:middle;display:inline-block;width:22px;height:18px;background-image:url(../images/btn_icon.png);background-repeat:no-repeat;}
.page_btn button i.fh_ico{background-position:-3px -260px;}
.page_btn button i.save_ico{background-position:-3px -300px;}
.page_btn button i.tj_ico{background-position:-2px -334px;}
.page_btn button i.pr_ico{background-position:-2px -365px;}
.page_btn button i.write{width:20px;background:url(../images/sys_icon.png) 0 -107px no-repeat;}
.bor{border:1px solid #d5dff4;line-height:22px;height:22px;}
.tips{font-size:12px;color:#4f648f;line-height:1.8;margin-top:20px;}
.tips dt{font-weight:bold;}
.rework_info{background:#ffffff;border:1px solid #ddd;margin:20px 0 0;padding:5px;font-size:12px;}
.rework_info dt label,.rework_info dd label{color:#8d8d8d;}
.rework_info dt,.rework_info dd{color:#0d4987;line-height:2;padding-left:8px;}
.rework_info dt{border-bottom:1px solid #dddddd;padding-bottom:5px;}
.rework_info dd{padding-top:5px;}
.rework_info dt strong,.rework_info dd strong{font-weight:bold;padding:0 6px;}
.rework_info dt strong.red{padding-right:50px;}
/*order_table validate*/
.order_table .textbox{width:99.5%!important;height:auto!important;}
.order_table .combo{border:0px!important;}
.order_table .validatebox-text{border:0px!important;}
