.news {
	position: relative;
	background-color: #fff;
	border-bottom: solid 1px #ECECEC
}

.news dt {
	padding: 1.5rem 0.9rem 1.5rem 5.6vw;
	display: inline-block;
}

.news dt div {
	width: 55px;
	border-radius: 3px;
	box-shadow: 0 0.5vw 2vw #c9dff1;
	color: #44b6ff;
	text-align: center;
	padding-top: 8px;
}
.list a:last-child .news {
	border-bottom: none
}

.news dt div span:first-child {
	font-size: 20px;
	display: block;
	font-weight: bold;
	line-height: 20px;
}

.news dt div span:last-child {
	font-size: 11px;
}

.news dd {
	position: absolute;
	display: inline-block;
	padding-top: 1.8rem;
	right: 2rem;
	left: 90px;
}

.news dd .newstit {
	line-height: 25px;
	font-size: 15px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #4A4A4A
}

.news dd .newstit+div {
	font-size: 12px;
	line-height: 20px;
}

.btn_blue {
	background-color: #13cea9;
	color: #fff;
}

.btn_orange {
	background-color: #FFB901;
	color: #fff;
}

.btn_block {
	width: 100%;
	line-height: 45px;
	border-radius: 3px;
	display: block;
	font-size: 1.4rem;
	text-align: center
}

.btn_sm {
	padding: 0 1rem;
	line-height: 28px;
	border-radius: 3px;
	display: inline-block;
	font-size: 1.3rem;
}

.tip_block {
	display: block;
	text-align: center
}

.tip_block i {
	font-size: 5rem;
}

.me dl {
	font-size: 1.3rem;
	background-color: #fff;
	height: 3.5rem;
	padding: 0 6vw;
}

.me dl a {
	height: 100%;
	width: 100%;
	border-bottom: solid 1px #DFDFDF;
	display: flex;
	display: -webkit-flex;
	align-items: center;
	justify-content: space-between;
	-webkit-align-items: center;
	-webkit-justify-content: space-between;
}
.me dl a.bang_ding {
	height: 100%;
	width: 100%;
	border-bottom: solid 1px #DFDFDF;
	display: flex;
	align-items: center;
	justify-content: flex-start;
	display: -webkit-flex;
	-webkit-align-items: center;
	-webkit-justify-content: flex-start
}
.me dl:last-child a{
	border-bottom: none;
}

.me dl dt {
	width: 10rem;
	text-align: left;
	color: #4a4a4a;
	display: flex;
	align-items: center;
	display: -webkit-flex;
	-webkit-align-items: center;
}
.me dl dt>span {
	margin-left: 2vw;
}

.me dl dd {
	text-align: right;
	color: #a8a8a8;
	display: flex;
	align-items: center;
	display: -webkit-flex;
	-webkit-align-items: center;
	padding-right: 1.9rem;

}

.me .after dd{padding-right: 0;}
.me .after dd span {margin-right: 0.6rem}
.me dl:last-child{
	padding-top: 3vw;
	height: auto;
	background-color: white;
	padding-bottom: 2vw;
}
.me dl:last-child a{
	height: auto;
	/*display: inline-block;*/
	display: flex;
	align-items: flex-start;
	display: -webkit-flex;
	-webkit-align-items: flex-start;
}
.me dl.tianxie-message:last-child{
	height: 3.5rem;
	background-color: white;
	box-sizing: content-box;
	padding-bottom: 6vw;
	padding-top: 0;

}
.me dl.tianxie-message:last-child a{
	height: 100%;
	display: flex;
	align-items: center;
	display: -webkit-flex;
	-webkit-align-items: center;
}
.me dl.tianxie-message dd{
	width: calc(100% - 2rem);
}
.me dl.tianxie-message dd input{
	outline:none;
}


.labels {
