	display: -ms-flexbox;
	display: flex;
	flex-flow: row wrap;
	-webkit-flex-flow: row wrap;
	overflow: hidden;
	justify-content: space-around;
	-webkit-justify-content: space-around;

}

.flex_inline .box a {
	display: flex;
