/*及时会话列表*/

.direct-chat-messages {
    -webkit-transform: translate(0, 0);
    -ms-transform: translate(0, 0);
    -o-transform: translate(0, 0);
    transform: translate(0, 0);
    padding: 10px 15px;
}

.direct-chat-msg,
.direct-chat-text {
    display: block
}

.direct-chat-msg {
    margin-bottom: 10px;
}

.direct-chat-msg:after,
.direct-chat-msg:before {
    content: " ";
    display: table
}

.direct-chat-msg:after {
    clear: both
}

.direct-chat-messages {
    -webkit-transition: -webkit-transform .5s ease-in-out;
    -moz-transition: -moz-transform .5s ease-in-out;
    -o-transition: -o-transform .5s ease-in-out;
    transition: transform .5s ease-in-out
}

.direct-chat-text {
    display: block;
    border-radius: 5px;
    position: relative;
    padding: 5px 5px;
    margin-left: 55px;
    background: #fff;
    border: 1px solid #fff;
    color: #8E8E8E;
    font-size: 13px;
}

.direct-chat-text:before {
    position: absolute;
    z-index: -1;
    right: 100%;
    top: 10px;
    border: solid transparent;
    border-right-color: #fff;
    content: ' ';
    height: 2px;
    width: 0;
    pointer-events: none;
}

.direct-chat-text:after {
    border-width: 5px;
    margin-top: -5px
}

.direct-chat-text:before {
    border-width: 6px;
    margin-top: -6px
}

.direct-chat-msg .direct-chat-img {
    float: left;
    width: 40px;
    height: 40px;
    border-radius: 50%;
}

.direct-chat-info {
    display: block;
    margin: 8px;
    font-size: .9em;
    text-align: center;
    clear: both;
}

.direct-chat-name {
    font-size: .9em;
    padding-left: 55px;
    display: block;
    margin-bottom: 5px;
    color: #818181;
}

.direct-chat-timestamp {
    color: #fff;
    background-color: #D7D7D7;
    padding: 2px 10px;
    border-radius: 5px;
    margin: 10px;
    font-size: 12px;
}

.direct-chat-msg.right .direct-chat-img {
    float: right
}

.direct-chat-msg.right .direct-chat-name {
    padding-right: 55px;
    padding-left: 0;
    text-align: right
}

.direct-chat-msg.right .direct-chat-text {
    margin-right: 55px;
    margin-left: 0;
    overflow: hidden;
}

.direct-chat-msg.right .direct-chat-text:before {
    right: 0;
    border-right-color: transparent;
    border-left-color: #fff;
    left: 100%
}

.direct-chat-msg.right .direct-chat-text .justify {
    display: inline-block;
    text-align: justify;
    float: right
}

