.talking-content>div{margin: 0.15rem 0;width: 100%}
.talking-content>div>section{padding: 0.2rem 0.3rem;max-width: 6rem;word-break: break-word;}
.talking-time div{margin: 0 auto; width: 1.71rem;height: 0.5rem; background-color: #e8e8e8;font-size: 0.24rem;color: #777;display: flex;justify-content: center;align-items: center;border-radius: 0.25rem;}

.talking-mine{color: white;font-size: 0.26rem}
.talking-mine section{background-color: #2abbb4;border-radius: 0.3rem;float: right;}
.talking-ta{color: #343434;font-size: 0.26rem}
.talking-ta section{background-color: white;border-radius: 0.3rem;float: left}
.talking-shuru{width: 100%;height: 17.5vh;font-size: 0;padding: 0 0.25rem 0 0.25rem}
/*.talking-shuru textarea{resize: none;width: 100%;font-size: 0.26rem;height: 1.26rem;padding: 0.25rem 0.25rem 0 0.25rem}*/
.talking-shuru article{border: none;width: 100%;font-size: 0.26rem;outline: none;height: 1.01rem;overflow-y: scroll;}
/*.max-H{height: 1.03rem;width: 100%;}*/

.sendtalking{font-size: 0.26rem;}
.talking-bottom{height: 0.93rem;display: flex;align-items: center;justify-content: space-between;padding: 0 0.05rem;}
.talking-bottom .icon{color: #2abbb4;font-size: 0.4rem;}
.icon-smile{margin-right: 0.5rem}
.sendtalking{width: 1rem;display: inline-block;padding: 0.05rem 0;text-align: center;color: #999;border-radius: 0.1rem}
.value-send{background-color: #2abbb4;color: white;}

.article-span{color: #999;}

/*处理空数据*/
.empty_img{width: 50%;margin: 30% 0 0 25%;}

