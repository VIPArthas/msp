*{box-sizing: border-box; padding:0; margin:0; list-style: none;font-family: 微软雅黑；}
.list-ul{ overflow: hidden;margin-top: 0.05rem;}
.list-li{ position:relative;
    -webkit-transform: translateX(0px);
}

a { text-decoration:none;}
a:active{-webkit-tap-highlight-color: transparent;}
a:hover{-webkit-tap-highlight-color: transparent;}
input,textarea{outline: none;border: none;font-family: 微软雅黑；}
::-webkit-input-placeholder { /* WebKit browsers */
    color: #999;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: #999999;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: #999;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: #999;
}
::-webkit-textarea-placeholder { /* WebKit browsers */
    color: #999;
}
:-ms-textarea-placeholder { /* Internet Explorer 10+ */
    color: #999;
}
/*聊一下*/
.btn{ position: absolute; top: 0; right: -160px; color: #fff; width: 160px;height: 100%}
.btn div{width: 100%;height: 100%;}
.btn span{line-height: 1.60rem;float: left; font-size: 0.30rem;display: inline-block;width: 80px;height: 100%;text-align: center;border: 0;padding: 0;margin: 0;}
@media only screen and (min-width: 321px) and (max-width: 376px) {
    .btn{right: -180px; width: 180px;}
    .btn span{width: 90px;}
}
@media only screen and (min-width: 376px) and (max-width: 415px) {
    .btn{right: -200px; width: 200px;}
    .btn span{width: 100px;}
}
.lahei{background-color: #a8a8a8;}
.del{background-color: #ff2e2e;}
.con{height: 1.60rem;display: flex;justify-content: flex-start;align-items: center;padding: 0 0.24rem;}
.con img{width: 1.02rem;margin-right: 0.18rem;border-radius: 50%;}
.liao-peopel{width:calc(100% - 1.5rem) }
