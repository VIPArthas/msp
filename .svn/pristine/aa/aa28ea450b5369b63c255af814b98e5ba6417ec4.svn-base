h4, h5, h6, h1, h2, h3 {
    margin: 0;
}
ul, ol {
    margin: 0;
}
p {
    margin: 0;
}
html, body {
    font-family: 'Î¢ÈíÑÅºÚ';
    font-size: 100%;
    background: #fff;
    margin: 0;
    height: 100%;

}
a {
    text-decoration: none;
}
a:active{
    text-decoration: #00ca86;
}
a:hover {
    transition: 0.5s all;
    -webkit-transition: 0.5s all;
    -moz-transition: 0.5s all;
    -o-transition: 0.5s all;
    text-decoration: none;
}
body {
    position: relative;
    /*width: 100%;*/
    /*height: 100%;*/
    min-width: 1500px;
    /*min-height:100%;*/
    /*min-width: 1000px;*/
    background-image: url("../img/backbj.jpg");
    background-repeat: no-repeat;
    background-size: cover;
    background-attachment: fixed;
}
.logoimg{
    position: absolute;
    left: 20vw;
    top: 171px;
}
section{
    position: absolute;
    right: 15vw;
    top: 18vh;
    width: 440px;
}
.div{
    width: 364px;
    margin: 0 auto;
    background-color:#efefef;
    border-radius: 10px;
    height: 364px;
}
h2{
    text-align: center;
    color: white;
    font-size: 40px;
    font-weight: 500;

    margin-bottom: 60px;
}
.h3{
    position: absolute;
    top:0;
    width: 364px;
    color: #73767e;
    font-size: 26px;
    text-align: center;
    line-height: 62px;

}
.title{
    height: 62px;
