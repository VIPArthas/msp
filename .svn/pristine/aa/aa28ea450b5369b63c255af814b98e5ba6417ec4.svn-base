}
.bactop{
    height: 31px;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    background-color: #3e424b;
}
.bacbot{
    height: 31px;
    background-color: #333740;
}

.form{
    width: 300px;
    margin: 0 auto;
}
.main{
    margin: 40px auto 45px auto ;
    height: 108px;
    width: 256px;
    background-color: white;
    padding: 0 22px;
}
.main div{
    height: 54px;
    line-height: 54px;
}
.adminname{border-bottom: 1px #efefef solid}
.main img{
    height: 20px;
}
.form input{
    /*margin: 0 auto;*/
